<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Frame" id="frm_main" named="True">
    <property name="background">#bdc3c7</property>
    <property name="height">200</property>
    <property name="width">300</property>
    <layout manager="pack">
      <property name="side">top</property>
    </layout>
    <child>
      <object class="tk.Button" id="btn_click_me" named="True">
        <property name="activebackground">#95a5a6</property>
        <property name="activeforeground">#FFFFFF</property>
        <property name="background">#7f8c8d</property>
        <property name="command" type="command" cbtype="simple">on_click_me</property>
        <property name="cursor">hand2</property>
        <property name="font">{Ubuntu Mono} 12 {bold}</property>
        <property name="foreground">#FFFFFF</property>
        <property name="highlightthickness">0</property>
        <property name="overrelief">flat</property>
        <property name="relief">flat</property>
        <property name="text" translatable="yes">Click Me !</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="height">30</property>
          <property name="relx">0.5</property>
          <property name="rely">0.5</property>
          <property name="width">150</property>
          <property name="x">-75</property>
          <property name="y">-15</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
